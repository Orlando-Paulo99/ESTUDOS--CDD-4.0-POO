from biblioteca import Forma,Triangulo, Retangulo, Atleta,Corredor,Nadador,Ciclista

#caculo da droma
f1=Forma(10,10)
print(f"o valor da base do perimetro é {f1.perimetro} e o valor da base da  area é {f1.area}")
#calculando a area do triangulo
f2=Triangulo(10,10)
f2.calculaArea(2,2)
#calculando a area do retangulo
f3=Retangulo(10,10)
f3.calcularPerimetro(4,4)
